{
  "name": "ers-utils",
  "version": "0.1.1",
  "description": "European Respiratory Society utils library for preparing and parsing content",
  "main": "lib/library.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "dev-travis": "webpack --env dev",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "test": "npm run eslint && nyc --reporter=lcov --reporter=text mocha --compilers js:babel-core/register --colors ./test/lib --recursive",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/lib --recursive",
    "test:watch-composition": "mocha --compilers js:babel-core/register --colors -w ./test/lib/composition.test.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "mocha": "mocha test/lib --recursive",
    "mocha:watch": "mocha test/ --recursive --watch",
    "mocha:debug": "mocha test/lib/composition.test.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EuropeanRespiratorySociety/ers-utils.git"
  },
  "keywords": [
    "utils",
    "ERS",
    "European",
    "Respiratory",
    "Society"
  ],
  "author": "Samuel Pouyt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EuropeanRespiratorySociety/ers-utils/issues"
  },
  "homepage": "https://github.com/EuropeanRespiratorySociety/ers-utils#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "6.24.1",
    "chai": "3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "istanbul": "^0.4.5",
    "mocha": "3.3.0",
    "nyc": "^11.1.0",
    "webpack": "^3.5.5",
    "yargs": "^8.0.2"
  },
  "dependencies": {
    "embed-video": "^1.4.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "moment": "^2.18.1",
    "sanitize-html": "^1.14.1"
  }
}
